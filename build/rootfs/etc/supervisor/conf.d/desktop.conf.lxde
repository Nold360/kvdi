[group:y]
programs=wm,lxpanel,pcmanfm

[program:wm]
priority=15
command=/usr/bin/openbox
environment=DISPLAY=":10",HOME="/root",USER="root"

[program:lxpanel]
priority=15
directory=%HOME%
command=/usr/bin/lxpanel --profile LXDE
user=%USER%
environment=DISPLAY=":10",HOME="%HOME%",USER="%USER%"

[program:pcmanfm]
priority=15
directory=%HOME%
command=/usr/bin/pcmanfm --desktop --profile LXDE
user=%USER%
environment=DISPLAY=":10",HOME="%HOME%",USER="%USER%"
